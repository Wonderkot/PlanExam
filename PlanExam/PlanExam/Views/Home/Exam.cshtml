</head>
<body>
<div id="container" onclick="init()">
    <img id="pic" src="@Url.Content(@Model.Picture)" alt="Image" class="map" usemap="#ImageMapmapAreas"/>
    <map id="ImageMapmapAreas" name="ImageMapmapAreas">
        <area id="poly" shape="rect" coords="122,191,428,410" alt="text" title="text" href="#">
    </map>
</div>
    <input id="x" hidden/>
    <input id="x2" hidden/>
    <input id="y" hidden/>
    <input id="y2" hidden/>
    <script src="/Scripts/utils.js" type="text/javascript"></script>
    <script src="/Scripts/draw.js" type="text/javascript"></script>

</body>
</html>
